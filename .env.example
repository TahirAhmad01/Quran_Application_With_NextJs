# Quran.Foundation OAuth configuration
# Select environment: 'prelive' or 'live'
QF_ENV=live

# Pre-Production (Test)
QF_PRELIVE_CLIENT_ID=
QF_PRELIVE_CLIENT_SECRET=
QF_PRELIVE_OAUTH_URL=https://prelive-oauth2.quran.foundation

# Production (Live)
QF_LIVE_CLIENT_ID=
QF_LIVE_CLIENT_SECRET=
QF_LIVE_OAUTH_URL=https://oauth2.quran.foundation

# Optional: Set a custom Content API base
# Defaults: prelive -> https://apis-prelive.quran.foundation, live -> https://apis.quran.foundation
# QF_CONTENT_BASE_URL=
